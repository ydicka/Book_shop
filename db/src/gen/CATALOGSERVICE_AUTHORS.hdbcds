using BE_TVC_LOGISTIC_AUTHORS as BE_TVC_LOGISTIC_AUTHORS;
using CATALOGSERVICE_BOOKS as CATALOGSERVICE_BOOKS;
view CATALOGSERVICE_AUTHORS as select from BE_TVC_LOGISTIC_AUTHORS as AUTHORS mixin {
  __BOOKS : association[*] to CATALOGSERVICE_BOOKS on __BOOKS.AUTHOR_ID = $projection.ID;
} into {
  AUTHORS.ID,
  AUTHORS.NAME,
  __BOOKS as BOOKS
};
