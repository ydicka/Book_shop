using BE_TVC_LOGISTIC_BOOKS as BE_TVC_LOGISTIC_BOOKS;
using CATALOGSERVICE_AUTHORS as CATALOGSERVICE_AUTHORS;
view CATALOGSERVICE_BOOKS as select from BE_TVC_LOGISTIC_BOOKS as BOOKS mixin {
  __AUTHOR : association to CATALOGSERVICE_AUTHORS on __AUTHOR.ID = $projection.AUTHOR_ID;
} into {
  BOOKS.ID,
  BOOKS.TITLE,
  __AUTHOR as AUTHOR,
  BOOKS.STOCK,
  BOOKS.AUTHOR_ID
};
